<?xml version="1.0" encoding="UTF-8"?>
<BasePage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="AppleMusicBrowser.AlbumListPage">
	<BasePage.Content>
		<ListView
			x:Name="albumListView"
			ItemsSource="{Binding AlbumList}">
			<ListView.ItemTemplate>
		        <DataTemplate>
		          <TextCell Text="Line" />
		        </DataTemplate>
		    </ListView.ItemTemplate>
		    <ListView.Footer>
		    	<Button Text="Debug" Clicked="OnDebugButtonClicked" />
		    </ListView.Footer>
      </ListView>
	</BasePage.Content>
</BasePage>
